[run]
branch = True
omit =
    */tests/*
    */venv/*
    */__pycache__/*

[report]
precision = 2
skip_covered = False
skip_empty = True

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
